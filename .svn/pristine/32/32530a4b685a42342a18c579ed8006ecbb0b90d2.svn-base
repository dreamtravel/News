#import "XWBosItem.h"

@implementation XWBosItem

+(instancetype)itemWithTitle:(NSString *)title image:(NSString *)image selectedImage:(NSString *)selectedImage
{
    XWBosItem *item=[[XWBosItem alloc]init];
    item.title=title;
    item.image=image;
    item.selectedImage=selectedImage;
    
    return item;
}


@end
